{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "incremental": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
  "@repo/shared": ["packages/shared/src"],
  "@repo/shared/*": ["packages/shared/src/*"],
  "@repo/prisma": ["packages/prisma"],
  "@repo/prisma/*": ["packages/prisma/*"],
  "@/auth": ["apps/web/src/auth.ts"],
  "@/lib/prisma": ["packages/prisma/src/client.ts"],
  "@/components/*": ["apps/web/components/*"],
  "@/lib/*": ["apps/web/lib/*"],
  "@/app/*": ["apps/web/app/*"],
  "@/src/*": ["apps/web/src/*"]
    },
    "allowJs": true,
    "esModuleInterop": true
  }
}
