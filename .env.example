# Database
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DB

# Redis (BullMQ)
REDIS_URL=rediss://default:PASSWORD@HOST:PORT/0

# Auth (Email)
AUTH_SECRET=replace-me
AUTH_EMAIL_SERVER_HOST=smtp.gmail.com
AUTH_EMAIL_SERVER_PORT=587
AUTH_EMAIL_SERVER_USER=you@example.com
AUTH_EMAIL_SERVER_PASS=app-password
AUTH_EMAIL_FROM=noreply@aiseoturbo.com
NEXTAUTH_URL=http://localhost:3000

# Google OAuth (used for both login AND Google Search Console)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Google Search Console (optional - will use GOOGLE_CLIENT_ID if not set)
# GSC_REDIRECT_URI=https://www.aiseoturbo.com/api/gsc/callback

# Optional
PSI_API_KEY=
FEATURE_PLAYWRIGHT_FALLBACK=false
